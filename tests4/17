class A {
  static var x = 10;
  static var y = 20;

  static function add(a, b) {
    return a + b;
  }

  static function add(a,b,c) {
    return a + b + c;
  }
}

class B extends A {
  static var x = 2;
  static var y = 30;

  static function add(a,b) {
    return a*b;
  }

  static function main() {
    return add(x,y) + add(x,x,x);
  }
}